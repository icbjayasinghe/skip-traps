{
  "expo": {
    "name": "skip-traps",
    "slug": "skip-traps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/nova-scotia-2-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/ns-background-3.webp",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
      "expo-router",
    {
      "origin": "https://skip-traps.expo.app",
    }],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ns-background-3.webp",
          "resizeMode": "cover",
          "backgroundColor": "#000000",
          "statusBarStyle": "light",
          "icon": "./assets/images/icon.png"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
